@page
@model Frontend.Pages.ListModel
@{
}

<h1>Listado de Saludos en Varios Idiomas</h1>

<table class="table">
    @foreach (var saludo in Model.Saludos)
    {
       <tr>
           <td>@saludo.EnEspanol</td>
           <td>@saludo.EnIngles</td>
           <td>@saludo.EnItaliano</td>
       </tr> 
    }
</table>
