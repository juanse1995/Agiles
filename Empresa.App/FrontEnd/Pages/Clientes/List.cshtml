@page
@model Frontend.Pages.ListModelClientes
@{
}

<div class="text-center center">
    <h1>Listado de clientes</h1>
</div>

<br/>
<br/>
<br/>

<table class="table">
    <tr>
        <th>ID</th>
        <th>TELEFONO</th>
        <th>ID EMPRESA</th>
    </tr>

<table class="table">
   @foreach (var cliente in Model.Clientes)
    {
       <tr>
           <td>@cliente.Id</td>
           <td>@cliente.Telefono</td>
           <td>@cliente.EsCliente</td>
       </tr> 
    }
</table>
