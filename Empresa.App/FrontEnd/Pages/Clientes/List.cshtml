@page
@model Frontend.Pages.ListModelClientes
@{
}

<h1>Listado de Clientes</h1>


<table class="table">
   @foreach (var cliente in Model.Clientes)
    {
       <tr>
           <td>@cliente.Id</td>
           <td>@cliente.Telefono</td>
           <td>@cliente.EsCliente</td>
       </tr> 
    }
</table>
