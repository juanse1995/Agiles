@page
@model Frontend.Pages.ListModelDirectivos
@{
}

<div class="text-center center">
    <h1>Listado de directivos</h1>
</div>

<br/>
<br/>
<br/>

<table class="table">
    <tr>
        <th>ID</th>
        <th>CATEGORIA</th>
        <th>ID SUBORDINADO</th>
        <th>DIRIGE EMPRESA</th>
    </tr>


<table class="table">
   @foreach (var directivo in Model.Directivos)
    {
       <tr>
           <td>@directivo.Id</td>
           <td>@directivo.Categoria</td>
           <td>@directivo.Subordinado</td>
           <td>@directivo.Dirige</td>         
       </tr> 
    }
</table>
